{
    "name": "acseo/typesense-bundle",
    "description": "This bundle provides integration with Typesense in Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Potier",
            "email": "nicolas.potier@acseo.fr"
        }
    ],    
    "require": {
        "php": "^7.1.3||^8.0",
        "doctrine/orm": "~2.4,>=2.4.5",
        "symfony/framework-bundle": "^3.4|^4.3|^5",
        "symfony/http-client-contracts": "^1.0|^2.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": { "ACSEO\\TypesenseBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "ACSEO\\TypesenseBundle\\Tests\\": "tests/"
        }
    }
}
